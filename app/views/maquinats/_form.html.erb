<%= form_with(model: maquinat, local: true) do |form| %>
  <% if maquinat.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(maquinat.errors.count, "error") %> prohibited this maquinat from being saved:</h2>

      <ul>
      <% maquinat.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>



 <div class="field">
    <%= form.label "Consorcio" %>
    <%= form.text_field :consorcio, id: :maquinat_consorcio %>
  </div>


   <div class="field">
    <%= form.label "Sucursal (debe ser minuscula)" %>
    <%= form.text_field :sucursal, id: :maquinat_sucursal %>
  </div>

   <div class="field">
    <%= form.label "usuarioventa (el mismo de sucursal)" %>
    <%= form.text_field :usuarioventa, id: :maquinat_usuarioventa %>
  </div>

   <div class="field">
    <%= form.label "Supervisor" %>
    <%= form.text_field :supervisor, id: :maquinat_supervisor %>
  </div>

   <div class="field">
    <%= form.label "Entrada" %>
    <%= form.text_field :entrada, id: :maquinat_entrada %>
  </div>

   <div class="field">
    <%= form.label "Salida" %>
    <%= form.text_field :salida, id: :maquinat_salida %>
  </div>





  <div class="field">
    <%= form.label :tipomaquinat_id %>
    <%= form.select :tipomaquinat_id, Tipomaquinat.order(:tipomaquina).collect{ |item|  [ " --> " + item.tipomaquina , item.id ] } %> 
   
  </div>

   


  <div class="field">
    <%= form.label "Descripcion y # Maquina" %>
    <%= form.text_field :descripcion, id: :maquinat_descripcion %>
  </div>

  
   <div class="field">
    <%= form.label "Serial" %>
    <%= form.text_field :serial, id: :maquinat_serial %>
  </div>


  <div class="field">
    <%= form.label "Activa" %>
    <% form.text_field :activa, id: :maquinat_activa %>
    <%= form.select :activa, ["si", "no"].collect{ |item|  [ item , item ] } %> 

  </div>

